{
    "name": "wordle-clone",
    "version": "0.1.0",
    "private": true,
    "homepage": "http://deathlybower959.github.io/react-wordle-clone",
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "5.0.0",
        "recharts": "^2.1.9",
        "styled-components": "^5.3.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "npm run build && echo thereciperack.ml >> build/CNAME",
        "deploy": "gh-pages -d build",
        "preregDeploy": "npm run build && if (Test-Path ./build/CNAME) {Remove-Item ./build/CNAME}",
        "regDeploy": "gh-pages -d build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "gh-pages": "^3.2.3"
    }
}
