{
    "name": "wordle-clone",
    "version": "0.1.0",
    "private": true,
    "homepage": "http://deathlybower959.github.io/react-wordle-clone",
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "5.0.0",
        "recharts": "^2.1.9",
        "styled-components": "^5.3.3"
    },
    "scripts": {
        "react:start": "react-scripts start",
        "react:build": "react-scripts build",
        "react:test": "react-scripts test",
        "react:eject": "react-scripts eject",
        "predeploy:custom": "npm run build -and echo exampledomain.ml >> build/CNAME",
        "deploy:custom": "gh-pages -d build",
        "predeploy:normal": "npm run build -and if (Test-Path ./build/CNAME) {Remove-Item ./build/CNAME}",
        "deploy:normal": "gh-pages -d build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "gh-pages": "^3.2.3"
    }
}
